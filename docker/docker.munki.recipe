<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Description</key>
    <string>Downloads the current release version of Docker for Mac  and imports into Munki.</string>
    <key>Identifier</key>
    <string>com.github.jarias.munki.docker</string>
    <key>Input</key>
    <dict>
      <key>NAME</key>
      <string>Docker</string>
      <key>MUNKI_REPO_SUBDIR</key>
      <string>dockern</string>
      <key>pkginfo</key>
      <dict>
        <key>catalogs</key>
        <array>
          <string>testing</string>
        </array>
        <key>description</key>
        <string>BUILD, SHIP, RUN</string>
        <key>display_name</key>
        <string>Docker</string>
        <key>name</key>
        <string>%NAME%</string>
        <key>category</key>
        <string>Development</string>
        <key>unattended_install</key>
        <true/>
      </dict>
    </dict>

    <key>Process</key>
    <array>
      <dict>
        <key>Processor</key>
        <string>URLDownloader</string>
        <key>Arguments</key>
        <dict>
          <key>url</key>
          <string>https://download.docker.com/mac/beta/Docker.dmg</string>
        </dict>
      </dict>
      <dict>
        <key>Processor</key>
        <string>MunkiImporter</string>
        <key>Arguments</key>
        <dict>
          <key>pkg_path</key>
          <string>%pathname%</string>
          <key>repo_subdirectory</key>
          <string>%MUNKI_REPO_SUBDIR%</string>
        </dict>
      </dict>
    </array>
  </dict>
</plist>
